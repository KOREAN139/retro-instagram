{
  "name": "retro-instagram",
  "author": "Sanggu Han <korean139@gmail.com>",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "dependencies": {
    "@emotion/react": "^11.1.3",
    "@reduxjs/toolkit": "^1.2.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.7.0",
    "electron": "^8.1.0",
    "electron-better-ipc": "^0.8.0",
    "electron-is-dev": "^1.1.0",
    "instagram-private-api": "^1.39.1",
    "mousetrap": "^1.6.5",
    "node-sass": "^4.13.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-hook-form": "^5.0.3",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "typescript": "~3.7.2"
  },
  "scripts": {
    "react-start": "craco start",
    "react-build": "craco build",
    "react-test": "craco test",
    "react-eject": "react-scripts eject",
    "tsc-electron": "tsc public/*.ts",
    "start": "concurrently \"cross-env BROWSER=none SASS_PATH=src yarn react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "yarn react-build && electron-builder",
    "release": "yarn react-build && electron-builder --publish=always",
    "prettier": "prettier --write \"**/*.{ts,tsx,scss}\"",
    "lint": "eslint --fix src --ext .ts --ext .tsx",
    "stylelint": "stylelint --fix src/**/*.scss --syntax scss"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^5.6.4",
    "@types/classnames": "^2.2.10",
    "@types/mousetrap": "^1.6.3",
    "@types/node-sass": "^4.11.0",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "concurrently": "^5.1.0",
    "craco-alias": "^2.1.1",
    "cross-env": "^7.0.2",
    "electron-builder": "^22.4.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "husky": ">=4",
    "lint-staged": ">=10",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "wait-on": "^4.0.1"
  },
  "homepage": ".",
  "build": {
    "icon": "public/icon.png"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "stylelint --cache --syntax css-in-js",
      "prettier --write"
    ],
    "*.{js,css,md}": "prettier --write"
  }
}
